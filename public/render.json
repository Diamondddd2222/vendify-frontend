{
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/about", "dest": "/index.html" },
    { "src": "/contact", "dest": "/index.html" },
    { "src": "/signup", "dest": "/index.html" },
    { "src": "/login", "dest": "/index.html" },
    { "src": "/*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}